@using Proje.Models
@model IEnumerable<Profile>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Document</title>

</head>


<body>


    <nav class="navbar navbar-expand-lg navbar-dark py-3 fixed-top NavbarOfOurs">
        <div class="container">
            <a href="#" class="navbar-brand">Freelance Connect</a>

            <button class="navbar-toggler"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target="#navmenu">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navmenu">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item">
                        <a href="Profile.html" class="nav-link">Profile</a>
                    </li>
                    <li class="nav-item">
                        <a href="index.html" class="nav-link">Home page</a>
                    </li>
                </ul>
            </div>


        </div>
    </nav>

    <!--Holder-->
    <div class="container light-style flex-grow-1 mt-5 container-p-y">
        <div class="card overflow-hidden">
            <div class="row no-gutters row-bordered row-border-light">
                <div class="col-md-3 pt-0">
                    <div class="list-group list-group-flush account-settings-links">
                        <a class="list-group-item list-group-item-action active" data-toggle="list"
                           href="#account-general">General</a>
                        <a class="list-group-item list-group-item-action" data-toggle="list"
                           href="#account-change-password">Change password</a>
                        <a class="list-group-item list-group-item-action" data-toggle="list"
                           href="#account-social-links">Social links</a>
                        <a class="list-group-item list-group-item-action" data-toggle="list"
                           href="#PostedJobs">PostedJobs</a>
                        <a class="list-group-item list-group-item-action" data-toggle="list"
                           href="#AppliedJobs">ApliedJobs</a>
                        <a class="list-group-item list-group-item-action" data-toggle="list"
                           href="#ActiveJobs">ActiveJobs</a>
                    </div>
                </div>
                <!--GeneralSection-->
                <div class="col-md-9">
                    <div class="tab-content">
                        <div class="tab-pane  fade active show" id="account-general">
                            <div class="card-body media align-items-center">
                                <img src="ProfilePictureDefault.svg" alt
                                     class="d-block rounded-circle ui-w-80">
                                <div class="media-body mt-3 ml-4">
                                    <label class="btn btn-outline-primary">
                                        Upload new photo
                                        <input type="file" class="account-settings-fileinput">
                                    </label> &nbsp;
                                    <button type="button" class="btn btn-default md-btn-flat">Reset</button>
                                    <div class="text-light small mt-1">Allowed JPG, GIF or PNG. Max size of 800K</div>
                                </div>
                            </div>
                            <hr class="border-light m-0">
                            <div class="card-body">
                                <div class="form-group">
                                    <label class="form-label">Username</label>
                                    <input type="text" class="form-control mb-1" value="">
                                </div>
                                <div class="form-group">
                                    <label class="form-label">Name</label>
                                    <input type="text" class="form-control" value="">
                                </div>
                                <div class="form-group">
                                    <label class="form-label">E-mail</label>
                                    <input type="text" class="form-control mb-1" value="">
                                </div>

                                <div class="text-right mt-3">
                                    <button type="button" class="btn btn-primary">Save changes</button>&nbsp;
                                    <button type="button" class="btn btn-default">Cancel</button>
                                </div>

                            </div>
                        </div>
                        <!--PasswordSection-->
                        <div class="tab-pane fade" id="account-change-password">
                            <div class="card-body pb-2">
                                <div class="form-group">
                                    <label class="form-label">Current password</label>
                                    <input type="password" class="form-control">
                                </div>
                                <div class="form-group">
                                    <label class="form-label">New password</label>
                                    <input type="password" class="form-control">
                                </div>
                                <div class="form-group">
                                    <label class="form-label">Repeat new password</label>
                                    <input type="password" class="form-control">
                                </div>

                                <div class="text-right mt-3">
                                    <button type="button" class="btn btn-primary">Save changes</button>&nbsp;
                                    <button type="button" class="btn btn-default">Cancel</button>
                                </div>
                            </div>
                        </div>
                        <!--Gith-LinkedIn-WebsiteLink-->
                        <div class="tab-pane fade" id="account-social-links">
                            <div class="card-body pb-2">
                                <div class="form-group">
                                    <label class="form-label">Website</label>
                                    <input type="text" class="form-control" value>
                                </div>
                                <div class="form-group">
                                    <label class="form-label">LinkedIn</label>
                                    <input type="text" class="form-control" value>
                                </div>
                                <div class="form-group">
                                    <label class="form-label">Github</label>
                                    <input type="text" class="form-control" value>
                                </div>

                                <div class="text-right mt-3">
                                    <button type="button" class="btn btn-primary">Save changes</button>&nbsp;
                                    <button type="button" class="btn btn-default">Cancel</button>
                                </div>
                            </div>
                        </div>
                        <!--ApliedJobs-->
                        @{
                            if (Model != null)
                            {
                                foreach (var Data in Model)
                                {
                                    <div class="tab-pane fade" id="AppliedJobs">
                                        <div class="card-body pb-2">
                                            <div class="row align-items-center WorkCards col-12  ">
                                                <div class="col-2 col-md-2 col-lg-1  d-none d-sm-block">
                                                    <img src="Demon.png" alt="" class="img-fluid rounded-circle ">
                                                </div>

                                                <div class="col-12 col-md-7 col-lg-8 ">
                                                    <div class="col-12 col-md-7 col-lg-8"><h5>@Data.urun_adi</h5></div>
                                                    <div class="col-12 col-md-7 col-lg-8 w-100 truncate-text" id="TurnDivApliedJobs"><p>@Data.urun_aciklamasi</p></div>
                                                    <div class="col-12 col-md-7 col-lg-8"><p>@Data.urun_zaman</p></div>
                                                    <div class="col-12 col-md-7 col-lg-8"><p class="lead">@Data.urun_type</p></div>
                                                </div>

                                                <div class="col-12 col-md-3 col-sm-3 align-items-center justify-content-center">
                                                    <div class="text-center">
                                                        <h2 class="col-12 mt-2">@Data.urun_para</h2>
                                                        <h3 class="col-12">@Data.teklif_para</h3>



                                                        <div class="text-center">
                                                            <div class="col-12 col-md-12">
                                                                <div class="input-group mb-3">
                                                                    <span class="input-group-text">$</span>
                                                                    <input type="text" class="form-control" placeholder="Enter price" aria-label="Price" aria-describedby="basic-addon2">
                                                                    <span class="input-group-text">.00</span>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <a href="LearnMoreAndBid.html">
                                                            <button data-mdb-ripple-init type="button" class="btn btn-info ">
                                                                Update Bid
                                                            </button>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            }
                            <!--Posted jobs-->
                            if (Model != null)
                            {
                                foreach (var Data in Model)
                                {
                                    <div class="tab-pane fade" id="PostedJobs">
                                        <div class="card-body pb-2">
                                            <div class="row align-items-center WorkCards col-12  ">
                                                <div class="col-2 col-md-2 col-lg-1  d-none d-sm-block">
                                                    <img src="Demon.png" alt="" class="img-fluid rounded-circle ">
                                                </div>

                                                <div class="col-12 col-md-7 col-lg-8 ">
                                                    <div class="col-12 col-md-7 col-lg-8"><h5>Freelance Game Developer</h5></div>
                                                    <div class="col-12 col-md-7 col-lg-8 w-100 truncate-text" id="TurnDivPostedJobs"><p>A Freelance Game Developer designs and codes video games on a project basis. Working independently, they collaborate with clients to deliver customized game solutions, showcasing creativity and technical expertise. This role involves crafting engaging game experiences while meeting project timelines.</p></div>
                                                    <div class="col-12 col-md-7 col-lg-8"><p>2 hours ago*5 bids</p></div>
                                                    <div class="col-12 col-md-7 col-lg-8"><p class="lead">JS C# ComputerScience</p></div>
                                                </div>


                                                <div class="col-12 col-md-3 col-sm-3 align-items-center justify-content-center">
                                                    <div class="text-center">
                                                        <h2 class="col-12 mt-2">$120 is the price your entered.</h2>
                                                        <button data-mdb-ripple-init type="button" class="btn btn-info ">
                                                            Delete
                                                        </button>
                                                        <a href="applicantPick.html">
                                                            <button data-mdb-ripple-init type="button" class="btn btn-info ">
                                                                See the applicants
                                                            </button>
                                                        </a>

                                                        <a href="UpdateJobInfo.html">
                                                            <button data-mdb-ripple-init type="button" class="btn btn-info ">
                                                                Update
                                                            </button>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                }
                            }
                            <!--ActiveJobs-->
                            if (Model != null)
                            {
                                foreach (var Data in Model)
                                {
                                    <div class="tab-pane fade" id="ActiveJobs">
                                        <div class="card-body pb-2">
                                            <div class="row align-items-center WorkCards col-12  ">
                                                <div class="col-2 col-md-2 col-lg-1  d-none d-sm-block">
                                                    <img src="Demon.png" alt="" class="img-fluid rounded-circle ">
                                                </div>

                                                <div class="col-12 col-md-7 col-lg-8 ">
                                                    <div class="col-12 col-md-7 col-lg-8"><h5>Freelance Game Developer</h5></div>
                                                    <div class="col-12 col-md-7 col-lg-8 w-100 truncate-text" id="TurnDivPostedJobs"><p>A Freelance Game Developer designs and codes video games on a project basis. Working independently, they collaborate with clients to deliver customized game solutions, showcasing creativity and technical expertise. This role involves crafting engaging game experiences while meeting project timelines.</p></div>
                                                    <div class="col-12 col-md-7 col-lg-8"><p>2 hours ago*5 bids</p></div>
                                                    <div class="col-12 col-md-7 col-lg-8"><p class="lead">JS C# ComputerScience</p></div>
                                                </div>


                                                <div class="col-12 col-md-3 col-sm-3 align-items-center justify-content-center">
                                                    <div class="text-center">
                                                        <h2 class="col-12 mt-2">$100 is the price you accepted.</h2>
                                                        <button data-mdb-ripple-init type="button" class="btn btn-info ">
                                                            Make the payment
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                    </div>
                                }
                            }
                        }    
                    </div>


                        



                </div>
                <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
                <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
                <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/js/bootstrap.bundle.min.js"></script>
                <script type="text/javascript"></script>
                <script src="~/js/textturnicationsprofile.js"></script>
                <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

</body>
                                
